{
    "problem": {
        "name": "graph_coloring_problem",
        "benchmark": "gc_1",
        "move_generator": "change_color_mg"
    },
    "algorithm": {
        "name": "simulated_annealing",
        "max_steps_without_improvement": 100,
        "optimization_strategy": "min",
        "initial_temperature": 1000,
        "cooling_step": 0.001,
        "min_temperature": 1e-8,
        "on_optimum_strategy": "restart",
        "on_optimum_strategy_repeats": 100
    },
    "solver_config": {
        "max_iter": 10000,
        "time_limit": 60000
    },
    "visualization": {
        "enabled": true,
        "min_time_between_steps": 0.01
    },
    "algorithm_monitor": true
}